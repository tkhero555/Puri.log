<% if @suggest_meals.present? %>
  <ul class="border-2 rounded border-black ">
    <% @suggest_meals.each do |suggest_meal| %>
        <li class="w-50 flex items-center gap-x-3.5 py-2 px-3 text-sm text-gray-800 hover:bg-blue-200 focus:ring-2 focus:ring-blue-500 bg-base-100 text-base-content dark:bg-base-200 dark:text-base-content" role="option" data-autocomplete-value="<%= suggest_meal.meal_name %>" data-autocomplete-label="<%= suggest_meal.meal_name %>">
          <%= suggest_meal.meal_name %>
        </li>
    <% end %>
  </ul>
<% end %>